{
    "$schema": "https://typedoc.org/schema.json",
    "intentionallyNotExported": [
        "ComponentClass",
        "AbstractComponent",
        "ConverterComponent",
        "RendererComponent",
        "SORT_STRATEGIES",
        "_ModelToObject",
        "EventHooksMomento",
        "MarkedPlugin"
    ],
    "entryPoints": ["../src"],
    "entryPointStrategy": "Resolve",
    "excludeExternals": true,
    "excludeInternal": false,
    "excludePrivate": true,
    "treatWarningsAsErrors": false,
    "validation": {
        "notExported": true,
        "invalidLink": true,
        "notDocumented": false
    },
    "visibilityFilters": {
        "protected": false,
        "private": false,
        "inherited": true,
        "external": false,
        "@internal": false
    },
    "logLevel": "Verbose"
}
